" Setip vundle
" vundle
" {
if exists('vundle')

	filetype off

	" Setup vundle
	set rtp+=~./vim/bundle/vundle
	call vundle#rc()

	filetype plugin indent on

endif
"}


" local

call TrySource("~/.vim/.vimrc.plugins.local")
